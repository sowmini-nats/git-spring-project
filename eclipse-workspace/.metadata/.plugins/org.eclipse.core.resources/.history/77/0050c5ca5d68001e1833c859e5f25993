package demo;

public class CustomerReport {
	private Customer[] custList = new Customer[5];
	
	public void addCustomer(Customer c) {
		for(int i=0;i<custList.length;i++) {
			custList[i]=c;
		}
	}

	public void printList(){
		for(int i=0;i<custList.length;i++) {
			custList[i].display1();
		}
	}
}
