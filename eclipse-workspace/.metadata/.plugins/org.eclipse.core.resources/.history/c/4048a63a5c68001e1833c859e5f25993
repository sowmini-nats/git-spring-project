package demo;

public class Customer {
	private int custNo; 		
	private String custName;
	private String custAddr; 
	void init(){
		custNo=1;
		custName="Ram";
		custAddr="Chennai";
	}
	Customer(){
		custNo=0;
		custName="unknown";
		custAddr="unknown";
	}
	Customer(int custNo,String custName, String custAddr){
		this.custNo=0;
		this.custName="unknown";
		this.custAddr="unknown";
	}
	public void display1() {
		System.out.println("custNo: "+custNo+"\ncustName: "+custName+"\ncustAddr: "+custAddr);
	}
	public int getCustNo() {
		return custNo;
	}
	public String getCustName() {
		return custName;
	}
	public String getCustAddr() {
		return custAddr;
	}
	public void setCustNo(int custNo) {
		this.custNo = custNo;
	}
	public void setCustName(String custName) {
		this.custName = custName;
	}
	public void setCustAddr(String custAddr) {
		this.custAddr = custAddr;
	}
	public String toString() {
		return "custNo: "+custNo+"\tcustName: "+custName+"\tcustAddr: "+custAddr;
	}
	
}


 