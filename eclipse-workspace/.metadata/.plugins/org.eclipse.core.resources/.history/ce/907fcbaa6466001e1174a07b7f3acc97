package demo;
import java.util.ArrayList;
import java.util.List;

public class MyList {
	public static void main(String[] args) {
		List myList = new ArrayList();
		myList.add(10);
		System.out.println(myList);
		myList.add(1,"Sowmini");
		System.out.println(myList);
		myList.add("Nandakkumar");
		System.out.println(myList);
	}
}
